@model IEnumerable<SI2Proy.Models.LineaProduccion>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumeroLineaProduccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreLineaProduccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCreacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <th>
                @Html.DisplayFor(modelItem => item.NumeroLineaProduccion)
            </th>
            <td>
                @Html.DisplayFor(modelItem => item.NombreLineaProduccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCreacion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NumeroLineaProduccion">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NumeroLineaProduccion">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NumeroLineaProduccion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
