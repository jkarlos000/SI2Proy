
@{
    ViewData["Title"] = "EliminarAsociacion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h1>Eliminar Asociacion de Materia Prima a Inventario</h1>

<div>
    <form asp-action="EliminarAsociacion">
        <div class="form-group">
            <label class="control-label">Materias Primas actualmente con control de Inventario</label>
            <select name="AgregadoFk" class="form-control" asp-items="ViewBag.AgregadoFk"></select>
        </div>

        <div class="form-group">
            <input type="submit" value="Eliminar" class="btn btn-primary" />
        </div>
    </form>
</div>